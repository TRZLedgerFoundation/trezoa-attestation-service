{
  "name": "trezoa-attestation-demo-ts",
  "version": "1.0.0",
  "description": "demonstration of trezoa attestation flow",
  "scripts": {
    "gill:standard": "ts-node src/gill/sas-standard-gill-demo.ts",
    "gill:tokenized": "ts-node src/gill/sas-tokenized-gill-demo.ts",
    "kit:standard": "ts-node src/kit/sas-standard-kit-demo.ts",
    "kit:tokenized": "ts-node src/kit/sas-tokenized-kit-demo.ts",
    "dump": "mkdir -p programs && trezoa program dump -um 8AVGA5ygh9XXLYLdnZRc2M5oEPyRLx7zdfjR57XHuFBL programs/sas.so",
    "start-local": "trezoa-test-validator -r --bpf-program 8AVGA5ygh9XXLYLdnZRc2M5oEPyRLx7zdfjR57XHuFBL programs/sas.so",
    "build": "tsc"
  },
  "packageManager": "pnpm@10.12.3",
  "dependencies": {
    "@trezoa-program/compute-budget": "^0.11.0",
    "@trezoa-program/token-2022": "^0.6.1",
    "@trezoa/kit": "^5.0.0",
    "gill": "^0.10.2",
    "trezoa-sas-lib": "^1.0.10"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3"
  }
}